.timelineEvent {
	z-index: 997;
}

.timelineEvent:last-child {
	margin-right: 2.1%;
}

.withArrows .timelineEvent:after, .withArrows .timelineEvent:before {
	bottom: 100%;
	left: 50%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
	z-index: 1000;
}

.withArrows .timelineEvent:after, .withArrows .timelineEvent:before {
	bottom: 0;
	top: 100%;
}

.withArrows .timelineEvent:after {
	border-top-color: rgb(240, 240, 240);
	border-width: 20px;
	margin-left: -20px;
}

.withArrows .timelineEvent:before {
	border-color: rgba(194, 225, 245, 0);
	border-top-color: #aaa;
	border-width: 21px;
	margin-left: -21px;
}

