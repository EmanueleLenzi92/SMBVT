@import url("https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600|Arvo:700");

html {
	height: 100%;
	width: 100%;
}

body {
	height: 100%;
	width: 100%;
	font-size: 140%;
	z-index: 0;
	margin: 0;
	padding: 0;
	border: 0;
	overflow: auto;
}

#container {
	display: none;
	margin: 0 auto;
	height: 60%;
	width: 100%;
	z-index: 1;
	box-sizing: border-box;
	text-align: center;
}

.back {
	background: #f9f9f9;
	border: 1px solid #bbb;
	overflow-y: auto;
}

#controls {
	vertical-align: top;
	display: inline-block;
	text-align: center;
	margin: 0 .4% 0 .6%;
	width: 40%;
	height: 100%;
	padding: .6% .2% .3% .2%;
	background-color: rgb(240, 240, 240);
}

#workspace {
	vertical-align: top;
	text-align: left;
	display: inline-block;
	margin: 0 .6% 0 .4%;
	width: 40%;
	height: 100%;
	padding: .5% 1% .5% 1%;
	background-color: rgb(240, 240, 240);
}

#workspaceForm {
	margin: 0 auto;
	text-align: left;
}

#rightNav {
	display: inline-block;
	width: 7%;
	margin: 0;
	padding: 0;
	height: 100%;
}

#leftNav {
	display: inline-block;
	width: 7%;
	margin: 0;
	padding: 0;
	height: 100%;
}

.form-group {
	margin-bottom: 1.5% !important;
}

.timelineEvent:first-child {
	margin-left: 1.5%;
}

#timeline {
	display: block;
	float: left;
	width: 100%;
	height: 32%;
	overflow-x: auto;
	overflow-y: hidden;
	white-space: nowrap;
	border-width: 2px 2px 0 0;
	margin-top: 1%;
}

.timelineEvent .data:focus {
	outline: none;
}

.data, .data:hover, .data:active, .data:visited, .data:link {
	text-decoration: none !important;
	color: #222 !important;
}

.data {
	display: inline-block;
	border: 1px solid #bbb;
	font-family: sans-serif;
	text-align: center;
	line-height: 56px;
	height: 56px;
	margin: .8%;
	overflow: hidden;
	text-decoration: none;
	z-index: 998;
	background-color: white;
	cursor: move;
	vertical-align: middle;
}

.data:hover {
	position: relative;
	cursor: move;
}

#controls .data {
	opacity: 1;
	min-width: 120px;
	width: 31%;
}

#inputDiv .data {
	opacity: 1;
	min-width: 110px;
	max-width: 10%;
}

.timelineEvent .data {
	display: block !important;
	width: 90%;
	height: 30px;
	line-height: 30px;
	margin: 5px auto;
	opacity: 1 !important;
	cursor: pointer;
	text-decoration: none;
	max-width: 90%;
}

@keyframes spinner-loader {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

.spinner-loader:not(:required) {
	  animation: spinner-loader 1500ms infinite linear;
	  border-radius: .5em;
	  box-shadow: rgba(0, 0, 51, 0.3) 1.5em 0 0 0, rgba(0, 0, 51, 0.3) 1.1em 1.1em 0 0, rgba(0, 0, 51, 0.3) 0 1.5em 0 0, rgba(0, 0, 51, 0.3) -1.1em 1.1em 0 0, rgba(0, 0, 51, 0.3) -1.5em 0 0 0, rgba(0, 0, 51, 0.3) -1.1em -1.1em 0 0, rgba(0, 0, 51, 0.3) 0 -1.5em 0 0, rgba(0, 0, 51, 0.3) 1.1em -1.1em 0 0;
	  font-size: 50%;
	  width: 1em;
	  height: 1em;
	  margin: 1.5em;
	  overflow: hidden;
	  text-indent: 100%;
	  margin: 48% auto;
}

#controls .inner-shadow {
	box-shadow: inset 0px 0px 0 2px rgba(100, 0, 255, 0.7);
}

.editable br {
	display: none;
}

.forbidden {
	background-image: -webkit-linear-gradient(right bottom, #f9f9f9 0%, #f9f9f9 25%, #ccc 25%, #ccc 50%, #f9f9f9 50%, #f9f9f9 75%, #ccc 75%);
	background-size: 50px 50px;
}

.ui-helper-hidden-accessible {
	display: none;
}

.timelineEvent {
	display: inline-block;
	font-family: sans-serif;
	vertical-align: middle;
	width: 16%;
	height: 90%;
	border: 1px solid #bbb;
	padding: 1% 1% 0 1%;
	text-align: center;
	margin-right: 1%;
	overflow-y: auto;
	overflow-x: hidden;
	background-color: #f9f9f9;
	cursor: pointer;
	position: relative;
	white-space: normal;
	overflow: visible;
}

.timelineEvent .titleHeading {
	font-size: 120%;
	line-height: 125%;
	margin: 3% auto 2% auto;
	font-weight: bold;
	text-overflow: ellipsis;
    overflow-y: visible;
    overflow-x: hidden;
    height: auto;
    white-space: nowrap;
}

.timelineEvent .dateHeading {
	font-size: 100%;
	line-height: 105%;
	margin: 0 auto 1% auto;
	padding-bottom: 1%;
	font-weight: bold;
}

.dropdown {
	top: -2.7%;
}

.dropdown-menu {
	z-index: 100;
	background-color: rgb(240, 240, 240);
